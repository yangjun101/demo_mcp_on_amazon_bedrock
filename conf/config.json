	"mcpServers": {
		"cot": {
			"command": "npx",
			"args": [
			  "-y",
			  "@modelcontextprotocol/server-sequential-thinking"
			]
		},
		"local_fs": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"./docs"
			],
			"env": {},
			"description": "Local File System I/O",
			"status": 1
		},
		"db_sqlite": {
			"command": "uvx",
			"args": [
				"mcp-server-sqlite",
				"--db-path",
				"./docs/test.db"
			],
			"env": {},
			"description": "DB Sqlite CRUD",
			"status": 1
		}
	}
